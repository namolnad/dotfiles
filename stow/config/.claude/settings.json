{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "hooks": {
    "Notification": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "~/Developer/dotfiles/scripts/claude-notification.sh"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Write|Edit|MultiEdit",
        "hooks": [
          {
            "type": "command",
            "command": "jq -r '.tool_input.file_path' | { read file_path; ~/Developer/dotfiles/scripts/remove-trailing-whitespace.sh \"$file_path\"; }"
          }
        ]
      }
    ]
  },
  "feedbackSurveyState": {
    "lastShownTime": 1754086954960
  },
  "enabledPlugins": {
    "typescript-lsp@claude-plugins-official": true
  }
}
